app: /emacs/
title: /drafts/
-

flow text: key(ctrl-c ctrl-q)
attach file: key(ctrl-c ctrl-a)
add signature: key(f2)
sign off: insert("\n\n  -k.\n")
save draft: key(ctrl-c ctrl-d)
kill message: key(ctrl-c ctrl-k)
send message: key(ctrl-c ctrl-c)

go to to: key(ctrl-c ctrl-f ctrl-t)
go to blind: key(ctrl-c ctrl-f ctrl-b)
go to (see|sea): key(ctrl-c ctrl-f ctrl-c)
go to subject: key(ctrl-c ctrl-f ctrl-s)
